{
  "name": "sam_api_server",
  "version": "1.0.0",
  "main": "app.js",
  "repository": "https://github.com/hr-js/sam_api_app.git",
  "scripts": {
    "prod:http": "node ./app/app.js",
    "prod:http2": "NODE_HTTP=http2 node ./app/app.js",
    "dev:http": "NODE_ENV=DEVELOP node ./app/app.js",
    "dev:http2": "NODE_HTTP=http2 NODE_ENV=DEVELOP node ./app/app.js",
    "test:http": "NODE_ENV=TEST jest",
    "test:http2": "NODE_ENV=TEST NODE_HTTP=http2 jest",
    "coverage:http": "NODE_ENV=TEST jest --coverage"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "author": "t-kurihara",
  "license": "MIT",
  "dependencies": {
    "elasticsearch": "^14.1.0",
    "fastify": "^1.7.0",
    "fastify-swagger": "^0.14.0",
    "moment": "^2.20.1"
  },
  "devDependencies": {
    "editorconfig": "^0.15.0",
    "eslint": "^4.19.1",
    "eslint-plugin-jest": "^21.15.0",
    "jest": "^22.2.2",
    "minimist": "^1.2.0",
    "node-fetch": "^2.0.0"
  }
}
